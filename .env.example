# Copy to .env and set your keys
# Only EXPO_PUBLIC_* vars are exposed to the client at build time.

# Google Gemini API Key (required for crop recommendations)
EXPO_PUBLIC_GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"
EXPO_PUBLIC_GEMINI_MODEL="Your-Gemini-Model-Name-Here"

# Backend API base URL (optional)
# Physical device: http://YOUR-LAN-IP:8000
# Android emulator: http://10.0.2.2:8000
# iOS simulator: http://localhost:8000
EXPO_PUBLIC_API_URL="http://10.0.2.2:8000"
